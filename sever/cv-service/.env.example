# CV Service Environment Variables Example
# Copy this file to .env and set your values

# Basic Configuration
SERVER_PORT=8084

# Eureka Server Configuration
EUREKA_SERVER_URL=http://localhost:8761/eureka/

# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/jobready?createDatabaseIfNotExist=true
